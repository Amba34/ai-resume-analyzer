{
  "version": 2,
  "builds": [
    {
      "src": "next-frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "next-frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_BACKEND_URL": "@backend-url",
    "JWT_SECRET": "@jwt-secret"
  }
}
